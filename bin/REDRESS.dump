INSERT INTO content VALUES (1, 'manual', 'http://redress.lancs.ac.uk/overview.html', 'text/html', 'Overview of the ReDReSS Project.', '', 'This web page gives an overview of th ESRC / JISC funded ReDReSS Project.The main aim of the project is to raise awareness and accelerate the development of a new kind of computing and data infrastructure to support the increasingly national and global collaborations emerging in many areas of social science.  ', 'English', 'Cutajar Bezzina, Audrienne', 'Lancaster University', '', 0.52707592319830199, '2005-12-23', 'Paul Dolby', 0);
INSERT INTO content VALUES (2, 'manual', 'http://redress.lancs.ac.uk/Acronyms/', 'text/html', 'ReDReSS project - Grid Acronyms Help.', '', 'This web page forms part of the ReDReSS project web site.  There are many acronyms associated with GRID technology, too many to remember. This web page is provided as a reference source for these acronyms.  It offers an indexed list, with associated descriptions, of popular GRID Acronyms.', 'English', 'Cutajar Bezzina, Audrienne', 'Lancaster', '', 0.59066780953169595, '2005-12-23', 'Paul Dolby', 0);
