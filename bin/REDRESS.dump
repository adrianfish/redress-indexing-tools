INSERT INTO category VALUES (1, 'Introductory Material and Support', 'An introduction to e-Social Science and the <a href="/Acronyms/acr_r.html#redress" target="_blank"><span class="glossary_link">ReDRESS</span></a> project and how this project fits into the support offered by the <a href="http://www.ncess.ac.uk" target="_blank"><span class="glossary_link">NCeSS</span></a> hub and nodes. <a href="http://en.wikipedia.org/wiki/E-Science" target="_blank"><span class="glossary_link">e-Science</span></a> technology is introduced, together with some of the organisations that are working to extend e-Science for Social Science research or are active in e-Science including NeSC. This section is very basic but includes links to related projects in other disciplines.   ', 1);
INSERT INTO category VALUES (2, 'Motivation and Background', 'A section describing why e-Social Science may provide new research opportunities or make existing research more efficient and effective. It contains illustrations of the limitations of conventional approaches to Social research, particularly in the area of large-scale social modeling. It shows examples of how e-Social Science technology has already been applied with some success to a number of projects and why it is promising and worthwhile to pursue for others. ', 2);
INSERT INTO category VALUES (9, 'Examples from e-Social Science Projects', 'Material about pilot demonstrator projects and examples.', 3);
INSERT INTO category VALUES (3, 'Agenda Setting Workshops', 'Ongoing JISC funded workshops.', 4);
INSERT INTO category VALUES (4, 'UK e-Science Programme', 'Links to material and support from the RCUK and JISC e-Science Programme.', 5);
INSERT INTO category VALUES (5, 'Collaboration', 'e-Collaboration technologies, e.g. Access Grid.', 6);
INSERT INTO category VALUES (6, 'Data Management', 'Data Management applied to qualitative, quantitative and combined data.', 7);
INSERT INTO category VALUES (8, 'Semantics, Knowledge and Information Management', 'Tools and technology for semantics, knowledge and information management.', 8);
INSERT INTO category VALUES (17, 'Databases', 'Relational and XML database technologies and their use.', 9);
INSERT INTO category VALUES (11, 'Social Shaping', 'Social Shaping of e-Science.', 10);
INSERT INTO category VALUES (7, 'Visualisation', 'Applying visualisation and VR techniques to better understanding of data', 11);
INSERT INTO category VALUES (10, 'Software Development', 'Issues of best practice and management.', 12);
INSERT INTO category VALUES (12, 'Portals and Related Technology', 'User interfaces for e-Science projects.', 13);
INSERT INTO category VALUES (13, 'High Performance Computing', 'Parallel computing and numerical algorithms to tackle social science grand challenges.', 14);
INSERT INTO category VALUES (14, 'Security and Confidentiality', 'Issues important to data handling and access to remote facilities.', 15);
INSERT INTO category VALUES (15, 'Grid and Web Services', 'Issues and technology for distributed computing and database access.', 16);
INSERT INTO category VALUES (16, 'Network Technology', 'Material related to using high-speed networks to facilitate e-Social Science', 17);


