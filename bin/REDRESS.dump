INSERT INTO content VALUES (137, 'manual', 'http://www.data-archive.ac.uk', 'text/html', 'UK Data Archive', 'esds , Economic and Social Data Service , The Data Archive, Data Archive, archive, data, computer-readable data, social sciences, humanities', 'The UK Data Archive (UKDA) is a centre of expertise in data acquisition, preservation, dissemination and promotion and is curator of the largest collection of digital data in the social sciences and humanities in the UK. It is funded by the Economic and Social Research Council (ESRC), the Joint Information Systems Committee (JISC) of the Higher Education Funding Councils and the University of Essex. Founded in 1967, it now houses several thousand datasets of interest to researchers in all sectors and from many different disciplines.', 'EN', 'Ageer, L', 'University of Essex', NULL, 0.47883930497131699, '2006-01-18', 'Dolby, Paul', 0);
INSERT INTO content VALUES (138, 'manual', 'http://nesstar.esds.ac.uk/webview/index.jsp', 'text/html', 'ESDS Nesstar Catalogue', 'nesstar', 'This site offers a searchable data library allowing you to locate, browse and download key economic and social data. For example: British General Election Study Data, British Social Attitudes Survey Data and Expenditure and Food Survey Data. ', 'EN', 'Nesstar', 'Nesstar', NULL, 0.26141584926470601, '2006-01-18', 'Dolby, Paul', 0);
INSERT INTO content VALUES (140, 'manual', 'http://redress.lancs.ac.uk/document-pool/An Overview of Internet and Grid Security - v2.html', 'text/html', 'An Overview of Internet and Grid Security', 'Distributed Environments, Security, Authentication, Authorisation, Confidentiality, Encryption, Decryption, Digital Certificates, Digital Signatures, Verification, Public Key, Grid, Globus', 'This Tutorial on Internet and Grid Security is provided in four  modules of increasing difficulty: Introduction to Distributed Environments and Security Terms, Description of Security Concepts and Components, Public Key Infrastructure, Grid Security Infrastructure.', 'EN', 'Baker, Mark', 'University of Portsmouth', NULL, 0.30427414470588199, '2006-01-18', 'Dolby, Paul', 0);


